{
    "editor.formatOnSave": false,
    // "terminal.integrated.profiles.osx": "/bin/zsh",
    // "terminal.integrated.inheritEnv": true,
    "files.encoding": "utf8",
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "cornflakes.linter.executablePath": "/usr/local/Caskroom/miniconda/base/bin/flake8",
    "editor.detectIndentation": false,
    "editor.renderWhitespace": "all",
    "files.insertFinalNewline": true,
    "editor.fontFamily": "Monaco",
    // "editor.fontFamily": "Source Code Pro",
    "editor.fontSize": 14,
    "files.hotExit": "onExit",
    "editor.matchBrackets": "always",
    "editor.tabSize": 4,
    "editor.trimAutoWhitespace": true,
    "files.trimTrailingWhitespace": true,
    "editor.wordWrap": "on",
    "editor.formatOnPaste": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "workbench.colorTheme": "Mariana Pro (Cool)",
    "latex-workshop.view.pdf.viewer": "tab",
    "explorer.confirmDragAndDrop": false,
    "workbench.iconTheme": "eq-material-theme-icons-light",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    // "eslint.autoFixOnSave": true,
    "eslint.alwaysShowStatus": true,
    // "prettier.disableLanguages": [
    //     "js"
    // ],
    "sync.gist": "62015269b9fa1216fdb9c5de2af1f679",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "git.ignoreLegacyWarning": true,
    "aws.samcli.location": "/usr/local/bin/sam",
    "terminal.external.linuxExec": "iterm",
    "terminal.external.osxExec": "iterm.app",
    "window.zoomLevel": 2,
    // "[markdown]": {
    //     "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    // },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "[markdown]": {
        "editor.quickSuggestions": {
        "other": true,
        "comments": true,
        "strings": true
        },
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
        // "editor.defaultFormatter": "mervin.markdown-formatter"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.python",
        "editor.formatOnType": true
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "hadolint.hadolintPath": "/opt/homebrew/bin/hadolint",
    "editor.stickyScroll.enabled": true,
    "[scss]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    }
}
