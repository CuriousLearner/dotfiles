{
    "editor.formatOnSave": false,
    // "terminal.integrated.profiles.osx": "/bin/zsh",
    // "terminal.integrated.inheritEnv": true,
    "files.encoding": "utf8",
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "cornflakes.linter.executablePath": "/usr/local/Caskroom/miniconda/base/bin/flake8",
    "editor.detectIndentation": true,
    "editor.renderWhitespace": "all",
    "files.insertFinalNewline": true,
    "editor.fontFamily": "Monaco",
    // "editor.fontFamily": "Source Code Pro",
    "editor.fontSize": 14,
    "files.hotExit": "onExit",
    "editor.matchBrackets": "always",
    "editor.tabSize": 4,
    "editor.trimAutoWhitespace": true,
    "files.trimTrailingWhitespace": true,
    "editor.wordWrap": "on",
    "editor.formatOnPaste": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "workbench.colorTheme": "Mariana Pro (Cool)",
    "latex-workshop.view.pdf.viewer": "tab",
    "explorer.confirmDragAndDrop": false,
    "workbench.iconTheme": "eq-material-theme-icons-light",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    // "eslint.autoFixOnSave": true,
    "eslint.alwaysShowStatus": true,
    // "prettier.disableLanguages": [
    //     "js"
    // ],
    "sync.gist": "62015269b9fa1216fdb9c5de2af1f679",
    // "editor.codeActionsOnSave": {

    //     "source.fixAll.eslint": "explicit"
    // },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "git.ignoreLegacyWarning": true,
    "aws.samcli.location": "/usr/local/bin/sam",
    "terminal.external.linuxExec": "iterm",
    "terminal.external.osxExec": "iterm.app",
    "window.zoomLevel": 2,
    // "[markdown]": {
    //     "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    // },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "[markdown]": {
        "editor.quickSuggestions": {
        "other": true,
        "comments": true,
        "strings": true
        },
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
        // "editor.defaultFormatter": "mervin.markdown-formatter"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnType": true
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "hadolint.hadolintPath": "/opt/homebrew/bin/hadolint",
    "editor.stickyScroll.enabled": true,
    "[scss]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "editor.inlineSuggest.showToolbar": "always",
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false
    },
    "workbench.editorAssociations": {
        "*.txt": "default"
    },
    "go.toolsManagement.autoUpdate": true,
    "inference.delay": 0,
    "notebook.includeCellOutputs": true,
    "continue.enableTabAutocomplete": true,
    "continue.telemetryEnabled": false,
    "dart.flutterSdkPath": "/Users/$(whoami)/Downloads/flutter",
    "workbench.sideBar.location": "right",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "salt-lint.enable": true,
    "salt-lint.run": "onType",
    "salt-lint.executablePath": "salt-lint",
    "update.showReleaseNotes": false,
    "continue.showInlineTip": false,
    "[sql]": {
        "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
    },
    "github.copilot.nextEditSuggestions.enabled": true,
    "yaml.schemas": {
        "file:///Users/sanyamkhurana/.vscode/extensions/continue.continue-1.3.16-darwin-arm64/config-yaml-schema.json": [
            ".continue/**/*.yaml"
        ]
    },
    "docker.extension.enableComposeLanguageServer": false,
    "workbench.editor.empty.hint": "hidden",
    "python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "gitlens.ai.model": "vscode",
    "gitlens.ai.vscode.model": "copilot:gpt-4.1",
}
